---
import { SITE_DESCRIPTION } from "config";
import BaseSEO from "./Base.astro";

type Props = {
  title: string;
  description?: string;
};

const { title, description = SITE_DESCRIPTION } = Astro.props;
---

<BaseSEO
  title={title}
  description={description}
  ogType="website"
  ogImage="xd"
/>
