{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@lib/*": ["lib/*"],
      "@styles/*": ["styles/*"],
      "@images/*": ["public/static/images/*"]
    },
    "types": ["@astrojs/image/client"]
  }
}
