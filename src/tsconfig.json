{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@lib/*": ["lib/*"],
      "@styles/*": ["styles/*"]
    },
    "types": ["@astrojs/image/client"]
  }
}
